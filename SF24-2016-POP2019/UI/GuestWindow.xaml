<Window x:Class="SF24_2016_POP2019.UI.GuestWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SF24_2016_POP2019.UI"
        mc:Ignorable="d"
        Title="Prozor za neprijavljenog korisnika" Height="450" Width="700">
    <Grid>
        <Grid.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                <GradientStop Color="#FF1010D4" Offset="0"/>
                <GradientStop Color="#FF1010D4" Offset="1"/>
                <GradientStop Color="White" Offset="0.518"/>
            </LinearGradientBrush>
        </Grid.Background>
        <DataGrid AutoGenerateColumns="True" SelectedItem="{Binding Path=IzabraniTermin}" Margin="12,12,12,57" Name="dgGuest" AutoGeneratedColumns="DgGuest_AutoGeneratedColumns" IsReadOnly="True" />
        <Button Content="Zatvori" Click="ZatvoriProzor_Click" Height="30" Margin="561,379,37,10"/>
        <Button Content="Login" Click="Login_Click" Height="30" Margin="12,379,586,10"/>
    </Grid>
</Window>
